<p id="notice"><%= notice %></p>

<h1>Listing Study Program Modes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_program_modes.each do |study_program_mode| %>
      <tr>
        <td><%= link_to 'Show', study_program_mode %></td>
        <td><%= link_to 'Edit', edit_study_program_mode_path(study_program_mode) %></td>
        <td><%= link_to 'Destroy', study_program_mode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

