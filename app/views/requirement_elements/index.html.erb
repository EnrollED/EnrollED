<p id="notice"><%= notice %></p>

<h1>Listing Requirement Elements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requirement_elements.each do |requirement_element| %>
      <tr>
        <td><%= link_to 'Show', requirement_element %></td>
        <td><%= link_to 'Edit', edit_requirement_element_path(requirement_element) %></td>
        <td><%= link_to 'Destroy', requirement_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Requirement element', new_requirement_element_path %>
