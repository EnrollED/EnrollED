<p id="notice"><%= notice %></p>

<h1>Listing Study Program Elements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_program_elements.each do |study_program_element| %>
      <tr>
        <td><%= link_to 'Show', study_program_element %></td>
        <td><%= link_to 'Edit', edit_study_program_element_path(study_program_element) %></td>
        <td><%= link_to 'Destroy', study_program_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

